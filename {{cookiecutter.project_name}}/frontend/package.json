{
  "name": "shoggoth",
  "version": "0.0.1",
  "description": "An opinionated react starterkit",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=development",
    "build:prod": "cross-env BABEL_ENV=production webpack --mode=production -p",
    "delete": "node bin/cli.js delete",
    "fixcode": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --write",
    "fixcode:test": "prettier \"app/**/*(*.css|*.js|*.json|*.md|*.scss)\" --list-different",
    "new": "node bin/cli.js new",
    "scaffold": "node bin/cli.js scaffold",
    "start": "node ./bin/dev-server",
    "styleguide": "styleguidist server",
    "test": "jest",
    "watch": "webpack --mode=development --watch"
  },
  "author": "Daniel Karlsson",
  "license": "MIT",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not dead"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./.setup-test.js"
    ],
    "testMatch": [
      "**/?(*.)(test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/internals/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "http://localhost"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-webpack-aliases": "^1.1.3",
    "commander": "^2.19.0",
    "copy-webpack-plugin": "^4.5.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-plugin-react": "^7.11.1",
    "expose-loader": "^0.7.5",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.2.0",
    "precss": "^3.1.2",
    "prettier": "1.14.3",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.11",
    "react-styleguidist": "^8.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "i18next": "^12.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
