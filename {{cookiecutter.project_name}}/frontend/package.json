{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@sentry/node": "^5.22.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.3",
    "babel-preset-react-app": "9.1.2",
    "blueimp-md5": "^2.18.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "esm": "^3.2.25",
    "hypernova": "^2.5.0",
    "hypernova-react": "^2.1.0",
    "i18next": "^19.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "react-scripts": "3.4.3",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.10"
  },
  "scripts": {
    "change-sass-to-prod": "sed -i 's/assetPath: $assetPathLocal/assetPath: $assetPathProd/g' src/styles/variables.scss",
    "change-sass-to-local": "sed -i 's/assetPath: $assetPathProd/assetPath: $assetPathLocal/g' $PWD/src/styles/variables.scss",
    "start": "PUBLIC_URL=/proxy npx react-scripts start",
    "start-ssl": "HTTPS=true PUBLIC_URL=/proxy npx react-scripts start",
    "build": "npm run change-sass-to-prod && PUBLIC_URL=/static/pipit/build npx react-scripts build && npm run change-sass-to-local",
    "test": "npx react-scripts test",
    "test:nowatch": "cross-env CI=true npx react-scripts test",
    "eject": "npx react-scripts eject",
    "postbuild": "rm -rf /webapp/pipit/static/pipit/build/ || true && cp -r build/ /webapp/pipit/static/pipit/build/",
    "hypernova": "node src/hypernovaBootstrap.js",
    "storybook": "npx start-storybook -h 0.0.0.0 -p 3001 -s public",
    "build-storybook": "npx build-storybook -s public",
    "new": "node cli/bin.js new"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-a11y": "^6.0.12",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^5.3.18"
  }
}
